{
  "name": "@remirror/extension-highlight",
  "version": "1.0.1",
  "private": true,
  "description": "Allow users to highlight important content",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-highlight",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-highlight"
  },
  "license": "MIT",
  "contributors": [
    "NEXT <open-source@nextapp.co>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-highlight.esm.js",
      "require": "./dist/remirror-extension-highlight.cjs.js",
      "browser": "./dist/remirror-extension-highlight.browser.esm.js",
      "types": "./dist/remirror-extension-highlight.cjs.d.ts",
      "default": "./dist/remirror-extension-highlight.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-highlight.cjs.js",
  "module": "dist/remirror-extension-highlight.esm.js",
  "browser": {
    "./dist/remirror-extension-highlight.cjs.js": "./dist/remirror-extension-highlight.browser.cjs.js",
    "./dist/remirror-extension-highlight.esm.js": "./dist/remirror-extension-highlight.browser.esm.js"
  },
  "types": "dist/remirror-extension-highlight.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.4.3"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.19"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  }
}
